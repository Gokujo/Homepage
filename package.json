{
  "name": "maxim-harder-homepage",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "vue-tsc --noEmit -p tsconfig.json",
    "build": "vue-tsc -b && vite build",
    "build:cloudflare": "vue-tsc -b && vite build --config vite.config.cloudflare.ts",
    "build:client": "vue-tsc -b && vite build --config vite.config.client.ts",
    "preview": "vite preview",
    "lint": "eslint --ext .ts,.vue src",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/vite": "^4.1.12",
    "@types/cloudflare-turnstile": "^0.2.2",
    "cfturnstile-vue3": "^2.0.0",
    "pinia": "^3.0.3",
    "vue": "^3.5.21",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.5",
    "@tailwindcss/postcss": "^4.1.13",
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "^8.9.0",
    "@typescript-eslint/parser": "^8.39.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.0.0",
    "@vue/eslint-config-typescript": "^14.0.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.33.0",
    "eslint-plugin-vue": "^10.4.0",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.6.3",
    "vite": "^7.1.2",
    "vue-tsc": "^3.0.5"
  }
}
